+	std::string result;
+	const char* propptr = jenv->GetStringUTFChars(property, 0);
+	bool r = db->GetProperty(Slice(propptr), &result);
+	jenv->ReleaseStringUTFChars(property, propptr);
+	return r ? jenv->NewStringUTF(result.c_str()) : 0;
+}
