+	virtual ~WindowsFileLock() { Close(); }
+	const std::string& GetFileName() { return fname_; }
+
+	bool Close() {
+		bool success = (file_ == INVALID_HANDLE_VALUE || CloseHandle(file_));
+		file_ = INVALID_HANDLE_VALUE;
